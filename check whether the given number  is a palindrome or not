#include<stdio.h>
int main()
{
    int n,i,rev,temp,a;
    printf("enter a number:");
    scanf("%d",&n);
    a=n;
    for(i=0;n>0;i++)
    {
        rev=n%10;
        temp=temp*10+rev;
        n=n/10;
    }
    if(a==temp)
    {
        printf(" it is a palindrome\n");
    }
    else
    {
        printf("it is not a palindrome\n");
    }
    return 0;
}
