#include<stdio.h>
int main()
{
 int arr[100][100],i,j,n,m;
 printf("enter the no of rows:");
 scanf("%d",&n);
 printf("enter the no of columns:");
 scanf("%d",&m);
 for(i=0;i<n;i++)
 {
     for(j=0;j<m;j++)
     {
         printf("enter the values:");
         scanf("%d",&arr[i][j]);
     }
 }   
 for(i=0;i<n;i++)
 {
     for(j=0;j<m;j++)
     {
         printf("%d",arr[i][j]);
     }printf("\n");
 }
 
   return 0;
   }
