#include<stdio.h>
#include<string.h>
void main()
{
    char a[10]="VIJAY";
    char b[10]="THALA";
    char c[10]="DHONI";
    for(int i=0;i<5;i++)
    {
            printf("  %c",a[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
            printf("  %c",b[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
            printf("  %c",c[i]);
        
    }
    printf("\n");
}
