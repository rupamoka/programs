#include<stdio.h>
void main()
{
    char name[10][10];
    int a;
    printf("Enter number of names:");
    scanf("%d",&a);
    for(int i=0;i<a;i++)
    {
        scanf("%s",name[i]);
    }
    printf("\n The names are follows.....");
    for(int i=0;i<a;i++)
    {
        printf("\nName%d.%s",i+1,name[i]);
    }
}
