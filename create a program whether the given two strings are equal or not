#include<stdio.h>
#include<string.h>
int main()
{
    char s1[30],s2[30];
    printf("\n enter the first string:");
    gets(s1);
    printf("\n enter the second string:");
    gets(s2);
    if(strcmp(s1,s2)==0)
    {
        printf("\n the given two strings are equal");
    }
    else
    {
        printf("\n the given two strings are not equal");
    }
    return 0;
}
